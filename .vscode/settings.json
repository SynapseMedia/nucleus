{
    "python.formatting.provider": "black",
    "cSpell.words": [
        "Algorand",
        "autouse",
        "coloredlogs",
        "creational",
        "dataclass",
        "ENOENT",
        "executescript",
        "ffinput",
        "ffprobe",
        "hexbytes",
        "hookwrapper",
        "imdb",
        "ipfs",
        "jsonify",
        "levelname",
        "muxing",
        "ORBITDB",
        "programname",
        "rtype",
        "strerror",
        "tqdm",
        "Tsize",
        "verboselogs"
    ],
    "python.testing.pytestArgs": [
        "."
    ],
    "python.analysis.typeCheckingMode": "strict",
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
    "python.experiments.optInto": [
        "All"
    ],
    "sqltools.connections": [
        {
            "previewLimit": 50,
            "driver": "SQLite",
            "name": "watchit.db",
            "database": "${workspaceFolder:watchit-toolkit}/watchit.db"
        }
    ],
    "sqltools.useNodeRuntime": true
}